<%@ Master Language="C#" Inherits="System.Web.Mvc.ViewMasterPage" %>
<%@ Import Namespace="StackExchange.DataExplorer" %>
<%@ Import Namespace="StackExchange.DataExplorer.Models" %>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
  <title>
    <asp:ContentPlaceHolder ID="TitleContent" runat="server" />
  </title>
  <% Site site = Current.Controller.Site; %>
  <%
      if (site != null)
      {%>
    <%=string.Format("<link rel=\"odata.service\" title=\"{0}\" href=\"{1}\"/>", site.Name,
                                          site.ODataEndpoint)%>
  <%
      }%>
  <%=AssetPackager.LinkCss("sitecss")%>

 
  
  <asp:ContentPlaceHolder ID="AdditionalStyles" runat="server" />
</head>
<body>
